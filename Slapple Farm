repeat wait() until game:IsLoaded()
local PlaceId = game.PlaceId
if PlaceId == 6403373529  then
    loadstring( game:HttpGet("https://raw.githubusercontent.com/Tuan122009/Slap-Battles/main/1"))()
elseif PlaceId == 9015014224 then
    loadstring( game:HttpGet("https://raw.githubusercontent.com/Tuan122009/Slap-Battles/main/2"))()
elseif PlaceId == 11520107397 then
    loadstring( game:HttpGet("https://raw.githubusercontent.com/Tuan122009/Slap-Battles/main/3"))()
end
